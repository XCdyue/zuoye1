<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.XC">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_xx"
        android:label="@string/my_name"
        android:roundIcon="@mipmap/ic_xx"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme"
        android:testOnly="false">
        <activity android:name="com.example.XC.WebActivity"></activity>
        <activity android:name="com.example.XC.EdtActivity" />
        <activity android:name="com.example.XC.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.XC.list.NetListActivity" />
        <activity android:name="com.example.XC.MainActivity">
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest>